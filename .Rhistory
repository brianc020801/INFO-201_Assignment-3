average_value_appi_over_years <- incarceration_df %>%
filter(year == 2018 | year == 1990) %>%
group_by(year) %>%
summarise(percentage = sum(aapi_pop_15to64) / sum(total_pop_15to64)) %>%
summarise(percent_change = max(percentage) / min(percentage) * 100) %>%
pull(percent_change)
